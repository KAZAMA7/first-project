<h1>Listing users</h1>
 
<table>
  <tr>
    <th>First name</th>
    <th>Last Name</th>
    <th>DOB</th>
    <th>Address</th>
    <th>Phone</th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.title %></td>
      <td><%= user.text %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
    </tr>
  <% end %>
</table>